<AnchorPane prefHeight="273.0" prefWidth="544.0" stylesheets="@CalcStyle.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LoginController">
   <children>
      <AnchorPane layoutX="5.0" layoutY="28.0" prefHeight="240.0" prefWidth="534.0" styleClass="MPane">
         <children>
            <Label layoutX="70.0" layoutY="64.0" text="User">
               <font>
                  <Font name="Times New Roman" size="20.0" />
               </font>
            </Label>
            <TextField fx:id="UserField" layoutX="66.0" layoutY="94.0" onAction="#LoginCheck" prefHeight="25.0" prefWidth="161.0" promptText="Enter Username">
               <font>
                  <Font name="Times New Roman" size="13.0" />
               </font>
            </TextField>
            <TextField fx:id="PassField" layoutX="247.0" layoutY="94.0" onAction="#LoginCheck" promptText="Enter Password" />
            <Button fx:id="LoginButton" layoutX="66.0" layoutY="129.0" mnemonicParsing="false" onAction="#LoginCheck" prefHeight="46.0" prefWidth="77.0" styleClass="BtnStyle" stylesheets="@CalcStyle.css" text="Login" />
            <Label layoutX="247.0" layoutY="64.0" text="Password">
               <font>
                  <Font name="Times New Roman" size="20.0" />
               </font>
            </Label>
            <Label fx:id="Success" layoutX="153.0" layoutY="141.0" prefHeight="23.0" prefWidth="133.0" />
